<?xml version="1.0" encoding="UTF-8"?>
<assembly xmlns="http://maven.apache.org/ASSEMBLY/2.1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.1.1 https://maven.apache.org/xsd/assembly-2.1.1.xsd">
    <id>${project.version}</id>
    <includeBaseDirectory>false</includeBaseDirectory>
    <formats>
        <format>jar</format>
    </formats>
    <files>
        <file>
            <source>README.md</source>
            <outputDirectory>META-INF</outputDirectory>
            <filtered>true</filtered>
        </file>
    </files>
    <fileSets>
        <fileSet>
            <directory>${project.build.directory}/classes</directory>
            <outputDirectory>/</outputDirectory>
        </fileSet>
    </fileSets>
    <dependencySets>
        <dependencySet>
            <unpack>true</unpack>
            <scope>compile</scope>
            <outputDirectory>/</outputDirectory>
            <useProjectArtifact>true</useProjectArtifact>
            <includes>
                <include>org.slf4j:jcl-over-slf4j:jar:</include>
                <include>javax.xml.bind:jaxb-api:jar:</include>
                <include>com.sun.xml.bind:jaxb-impl:jar:</include>
                <include>com.sun.xml.bind:jaxb-core:jar:</include>
                <include>com.fasterxml.woodstox:woodstox-core:jar:</include>
                <include>org.apache.tika:tika-core:jar:</include>
                <include>com.aliyun.oss:aliyun-sdk-oss:jar:</include>
                <include>com.alipay.sdk:alipay-sdk-java:jar:</include>
                <include>commons-logging:commons-logging:jar:</include>
                <include>org.slf4j:log4j-over-slf4j:jar:</include>
            </includes>
        </dependencySet>
    </dependencySets>
</assembly>